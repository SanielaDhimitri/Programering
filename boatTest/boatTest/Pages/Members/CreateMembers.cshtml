@page
@model boatTest.Pages.Members.CreateMembersModel

@{
}
<h1>Create a new Medlem</h1>
    <form method="post">
        <div class="form-group">
            <label asp-for="Member.Id">Id:</label>
            <input asp-for="Member.Id"class="form-control"/>
        <span asp-validation-for="Member.Id" class="text-danger"></span>
    </div>
        <div>
            <label asp-for="Member.Name">Name:</label>
            <input asp-for="Member.Name"class="form-control"/>
        <span asp-validation-for="Member.Name" class="text-danger"></span>
        </div>

        <div>
            <label asp-for="Member.Adresse">Adresse:</label>
            <input asp-for="Member.Adresse"class="form-control"/>
       <span asp-validation-for="Member.Adresse" class="text-danger"></span>
       </div>
        <div>
            <label asp-for="Member.Phone">Phone:</label>
            <input asp-for="Member.Phone" class="form-control"/>
        <span asp-validation-for="Member.Phone" class="text-danger"></span>
        </div>
    <div>
        <label asp-for="Member.Age">Age:</label>
        <input asp-for="Member.Age" class="form-control" />
        <span asp-validation-for="Member.Age" class="text-danger"></span>
    </div>
    <div class="row mt-3">
        <div class="col-12 col-sm-6 mb-2">
        <button type="submit" class="btn btn-primary w-100">Create</button>  @*"submit"=dorezoj *@ 
        </div>
        <div class="col-12 col-sm-6 mb-2">
            <a asp-page="/Members/GetAllMembers" class="btn btn-warning w-100">Back to List</a>
        </div>
    </div>
  
    </form>
