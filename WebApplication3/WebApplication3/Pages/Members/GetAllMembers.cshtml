@page
@using WebApplication3.Models
@model WebApplication3.Pages.Members.GetAllMembersModel
@{
}
<h1>Members</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>MemberId</th>
            <th>MemberName</th>
            <th>Adresse</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Member member in Model._members)
{
     <tr>
        <td>@member.MemberId</td>
        <td>@member.MemberName</td>
        <td>@member.Adresse</td>
        <td>@member.Phone</td>
    </tr>
}
    </tbody>
</table>
<a class="btn btn-primary" asp-page="/Members/CreateMembers">Create New Member</a>
