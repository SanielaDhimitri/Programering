@page
@using WebApplication3.Models
@model WebApplication3.Pages.Boats.GetAllMMembersModel
@{
}
<h1>Boats</h1>
<table class="table table-striped">
    <thead>
    <tr>
        <th>BoatId</th>
        <th>BoatName</th>
        <th>Type</th>      
    </tr>
    </thead>
    <tbody>
        @foreach(Boat boat in Model._boats)
       { <tr>
            <td>@boat.BoatId</td>
            <td>@boat.BoatName</td>
            <td>@boat.Type</td>
        </tr>
       }
    </tbody>
</table>
<a class="btn btn-primary" asp-page="/Boats/CreateBoats">Create New Boat</a>